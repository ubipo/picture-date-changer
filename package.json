{
  "name": "picture-date-changer",
  "version": "1.0.0",
  "description": "Change the date or time of multiple pictures at a time",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "bash ./dev.sh",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p src/electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubipo/picture-date-changer.git"
  },
  "keywords": [
    "picture",
    "EXIF",
    "date",
    "time",
    "datetime"
  ],
  "author": "Pieter & Tomas Fiers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ubipo/picture-date-changer/issues"
  },
  "homepage": "https://github.com/ubipo/picture-date-changer#readme",
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "electron": "^22.0.3",
    "eslint": "^8.32.0",
    "image-type": "^5.2.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "styled-components": "^5.3.6",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "dayjs": "^1.11.7",
    "piexifjs": "^1.0.6"
  }
}
